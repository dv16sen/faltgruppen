    "@types/mocha": "^5.2.5",
    "@types/supertest": "^2.0.6",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "supertest": "^3.3.0",
    "ts-mocha": "^2.0.0"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/csurf": "^1.9.35",
    "@types/express": "^4.16.0",
    "@types/glob": "^7.1.1",
    "@types/helmet": "0.0.42",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.9",
    "@types/sequelize": "^4.27.32",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "csurf": "^1.9.0",
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "helmet": "^3.15.0",
    "lodash": "^4.17.11",
    "mysql2": "^1.6.4",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.41.2",
    "sequelize-typescript": "^0.6.6",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.1"
  },
  "scripts": {
    "init": "npm install && cd client && npm install",
    "dev": "concurrently \"npm run startClient\" \"npm run startServer\"",
    "start": "cross-env NODE_ENV=production ts-node server/index.ts",
    "build": "cd client && npm run build",
    "test": "concurrently \"npm run testServer\" \"npm run testClient\"",
    "startServer": "nodemon",
    "testServer": "cross-env NODE_ENV=test ts-mocha server/test/**/*.ts",
    "startClient": "cd client && npm run start",
    "testClient": "cd client && npm run test",
    "updateRoutes": "ts-node scripts.ts updateRoutes",
    "removeDatabase": "ts-node scripts.ts removeDatabase",
    "defaultDatabase": "ts-node scripts.ts defaultDatabase",
    "heroku-postbuild": "npm run init && npm run build"
  }
}
